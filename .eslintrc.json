{
  "parser": "babel-eslint",
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:import/recommended"
  ],
    "settings": {
      "react": {
        "createClass": "createReactClass",
        "pragma": "React",  
        "version": "16.2.0"

      },
      "propWrapperFunctions": [ "forbidExtraProps" ]
    },
  "env": {
    "browser": true,
    "node": true
  },
  "plugins": [
    "import",
    "react"
  ]
}